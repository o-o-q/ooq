log.script("inp.script")

function init(_s)
	_s._id = go.get_id()
	pst.script(_s._id, "acquire_input_focus")
	extend.init(_s, Inp)
end

function update(_s, dt)
	_s:upd(dt)
end

function on_input(_s, key, keyact)
	_s:__(key, keyact)
	_s:on_input_player()
	_s:next()
end

function final(_s)
	pst.script(_s._id, "release_input_focus")
end
